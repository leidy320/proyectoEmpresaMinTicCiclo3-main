                <div class="col-4">

                </div>
                <div class="col-4">
                    <div class="form-group">
                        <label for="direccionEmpresa">Dirección</label>
                        <input disabled="true" type="text" class="form-control" id="direccionEmpresa" value="@Model.Empresa.Direccion">
                    </div>
                </div>
                <div class="col-4">

                </div>
            </div>
            <div class="row">
                <div class="col-4">

                </div>
                <div class="col-4">
                    <div>
                        <a class="btn btn-primary" asp-page="./List">
                            <i class="fas fa-arrow-left"></i>
                        </a>
                    </div>
                </div>
                <div class="col-4">

                </div>
            </div>
        </div>
